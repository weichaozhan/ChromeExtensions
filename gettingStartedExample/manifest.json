{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "starting.png"
  },
  "icons": {
    "48": "starting.png"
  },
  "permissions": ["storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
