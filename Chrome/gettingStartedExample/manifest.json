{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "permissions": ["activeTab", "tabs", "declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "starting.png",
      "32": "starting.png",
      "48": "starting.png",
      "128": "starting.png"
    }
  },
  "options_page": "option.html",
  "icons": {
    "16": "starting.png",
    "32": "starting.png",
    "48": "starting.png",
    "128": "starting.png"
  }
}
